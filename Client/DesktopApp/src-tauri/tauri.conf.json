{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Phong hop KGT",
  "version": "0.1.0",
  "identifier": "com.sonnh.desktopapp",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Hệ thống Hội nghị và Phòng họp Thông minh - Không giấy tờ",
        "minWidth": 500,
        "minimizable": false,
        "fullscreen": true,
        "closable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
